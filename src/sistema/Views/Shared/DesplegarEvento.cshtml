@model Eventos
<div class="card">
    <div class="col-lg-3 col-md-6 mb-4">
        <div class="card">
            <img class="card-img-top" src="@Model.codevento" alt="">
            <div class="card-body">
                <h4 class="card-title">@Model.nomevento</h4>
                <p class="card-text">@Model.desevento</p>
            </div>
            <div class="card-footer">
                <a href="#" class="btn btn-primary">Detalle Evento</a>
            </div>
        </div>
    </div>
</div>